<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Profile">

  <TextView
      android:id="@+id/text_view"
      android:layout_width="match_parent"
      android:layout_height="65dp"
      android:layout_alignParentTop="true"
      android:gravity="center_horizontal|bottom"
      android:text="@string/profile"
      android:textAppearance="@style/profile"
      android:textColor="#546E7A"
      android:layout_alignParentLeft="true"
      android:clipToOutline="true"

      android:elevation="5dp"/>

  <RelativeLayout
      android:id="@+id/centre_layout"
      android:layout_width="match_parent"
      android:layout_height="match_parent"
      android:layout_marginBottom="66dp"
      android:layout_below="@+id/text_view"
      >

    <RelativeLayout
        android:id="@+id/upper_layout"
        android:layout_width="match_parent"
        android:layout_height="91dp"
        android:layout_marginHorizontal="22dp"
        android:layout_marginTop="22dp">

      <ImageView
          android:id="@+id/imageButton"
          android:layout_width="83dp"
          android:layout_height="82dp"
          android:background="@drawable/circular_button_background"
          android:clipToOutline="true"
          android:contentDescription="@string/already_hav"
          android:layout_marginTop="6dp"
          android:scaleType="center"
          app:srcCompat="@drawable/man" />

      <RelativeLayout
          android:id="@+id/nestedRelativeLayout"
          android:layout_width="163dp"
          android:layout_height="64dp"
          android:layout_alignParentRight="true"
          android:layout_marginStart="8dp"
          android:layout_marginEnd="32dp"
          android:layout_marginRight="113dp"
          android:layout_toRightOf="@id/imageButton">


        <TextView
            android:id="@+id/hello_ken"
            android:layout_width="107dp"
            android:layout_height="wrap_content"
            android:layout_marginTop="22dp"
            android:lineSpacingExtra="11sp"
            android:text="@string/hello_ken"
            android:textAppearance="@style/hello_ken"
            android:translationY="-5.62sp" />

        <TextView
            android:id="@+id/current_bal"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentBottom="true"
            android:lineSpacingExtra="2sp"
            android:text="@string/current_bal"
            android:textAppearance="@style/current_bal"
            android:translationY="-0.97sp" />

        <TextView
            android:id="@+id/n10_712_00"
            android:layout_width="63dp"
            android:layout_height="16dp"
            android:layout_alignBaseline="@id/current_bal"
            android:layout_toRightOf="@id/current_bal"
            android:gravity="top"
            android:lineSpacingExtra="2sp"
            android:paddingStart="4dp"
            android:text="@string/balance"
            android:textAppearance="@style/balance"
            android:translationY="-0.97sp" />
      </RelativeLayout>

      <ImageButton
          android:id="@+id/imageButton2"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          app:srcCompat="@drawable/ic_pass_show"
          android:layout_alignParentRight="true"
          android:backgroundTint="@color/transparent_color"
          android:layout_marginTop="16dp"/>
    </RelativeLayout>

    <FrameLayout
        android:id="@+id/frame_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/upper_layout"
        android:layout_marginHorizontal="56dp"
        android:layout_marginTop="20dp">

      <TextView
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:text="@string/enable_dark"
          android:textAppearance="@style/enable_dark" />

      <Switch
          android:id="@+id/switch1"
          android:layout_width="wrap_content"
          android:layout_height="33dp"
          android:layout_gravity="end|top"
          android:gravity="top" />
    </FrameLayout>

    <LinearLayout
        android:id="@+id/linearLayout"
        android:layout_width="wrap_content"
        android:layout_height="401dp"
        android:layout_below="@+id/frame_layout"
        android:layout_marginHorizontal="26dp"
        android:layout_marginTop="12dp"
        android:layout_marginBottom="2dp"
        android:orientation="vertical">

      <RelativeLayout
          android:layout_width="match_parent"
          android:layout_height="44dp"
          android:layout_alignParentLeft="true"
          android:layout_alignParentTop="true"
          android:background="#FFFFFF"
          android:clipToOutline="true"
          android:elevation="5dp">

        <ImageView
            android:id="@+id/imageView5"
            android:layout_width="35dp"
            android:layout_height="32dp"
            app:srcCompat="@drawable/profile_circle" />

        <FrameLayout
            android:layout_width="285dp"
            android:layout_height="36dp"
            android:layout_marginLeft="8dp"
            android:layout_toRightOf="@id/imageView5">

          <TextView
              android:id="@+id/edit_profil"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:gravity="top"
              android:lineSpacingExtra="-3sp"
              android:text="@string/edit_profil"
              android:textAppearance="@style/edit_profil" />


          <TextView
              android:id="@+id/name_phone_"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_gravity="bottom"
              android:lineSpacingExtra="2sp"
              android:text="@string/name_phone_"
              android:textAppearance="@style/name_phone_"
              android:translationY="-0.97sp" />
        </FrameLayout>

        <ImageView
            android:id="@+id/imageView6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="6dp"
            app:srcCompat="@drawable/frame_106" />
      </RelativeLayout>

      <RelativeLayout
          android:layout_width="match_parent"
          android:layout_height="44dp"
          android:layout_alignParentLeft="true"
          android:layout_alignParentTop="true"
          android:layout_marginTop="12dp"
          android:background="#FFFFFF"
          android:clipToOutline="true"
          android:elevation="5dp">

        <ImageView
            android:id="@+id/imageView7"
            android:layout_width="35dp"
            android:layout_height="32dp"
            app:srcCompat="@drawable/peper" />

        <FrameLayout
            android:layout_width="285dp"
            android:layout_height="36dp"
            android:layout_marginLeft="8dp"
            android:layout_toRightOf="@id/imageView7">

          <TextView
              android:id="@+id/statements_"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:gravity="top"
              android:lineSpacingExtra="-3sp"
              android:text="@string/statements_"
              android:textAppearance="@style/statements_" />

          <TextView
              android:id="@+id/download_tr"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_gravity="bottom"
              android:lineSpacingExtra="2sp"
              android:text="@string/download_tr"
              android:textAppearance="@style/download_tr"
              android:translationY="-0.97sp" />
        </FrameLayout>

        <ImageView
            android:id="@+id/imageView8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="6dp"
            app:srcCompat="@drawable/frame_106" />
      </RelativeLayout>

      <RelativeLayout
          android:layout_width="match_parent"
          android:layout_height="44dp"
          android:layout_alignParentLeft="true"
          android:layout_alignParentTop="true"
          android:layout_marginTop="12dp"
          android:background="#FFFFFF"
          android:clipToOutline="true"
          android:elevation="5dp">

        <ImageView
            android:id="@+id/imageView9"
            android:layout_width="35dp"
            android:layout_height="32dp"
            app:srcCompat="@drawable/notification" />

        <FrameLayout
            android:layout_width="285dp"
            android:layout_height="36dp"
            android:layout_marginLeft="8dp"
            android:layout_toRightOf="@id/imageView9">

          <TextView
              android:id="@+id/notification"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:gravity="top"
              android:lineSpacingExtra="-3sp"
              android:text="@string/notificatio"
              android:textAppearance="@style/notificatio" />

          <TextView
              android:id="@+id/mute_unmute"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_gravity="bottom"
              android:lineSpacingExtra="2sp"
              android:text="@string/mute_unmute"
              android:textAppearance="@style/mute_unmute"
              android:translationY="-0.97sp" />
        </FrameLayout>

        <ImageView
            android:id="@+id/imageView10"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="6dp"
            app:srcCompat="@drawable/frame_106" />
      </RelativeLayout>

      <RelativeLayout
          android:layout_width="match_parent"
          android:layout_height="44dp"
          android:layout_alignParentLeft="true"
          android:layout_alignParentTop="true"
          android:layout_marginTop="12dp"
          android:background="#FFFFFF"
          android:clipToOutline="true"
          android:elevation="5dp">

        <ImageView
            android:id="@+id/imageView11"
            android:layout_width="35dp"
            android:layout_height="32dp"
            app:srcCompat="@drawable/wallet_2" />

        <FrameLayout
            android:layout_width="285dp"
            android:layout_height="36dp"
            android:layout_marginLeft="8dp"
            android:layout_toRightOf="@id/imageView11">

          <TextView
              android:id="@+id/card_bank_a"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:gravity="top"
              android:lineSpacingExtra="-3sp"
              android:text="@string/card_bank_a"
              android:textAppearance="@style/card_bank_a" />

          <TextView
              android:id="@+id/change_card"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_gravity="bottom"
              android:lineSpacingExtra="2sp"
              android:text="@string/change_card"
              android:textAppearance="@style/change_card"
              android:translationY="-0.97sp" />
        </FrameLayout>

        <ImageView
            android:id="@+id/imageView12"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="6dp"
            app:srcCompat="@drawable/frame_106" />
      </RelativeLayout>

      <RelativeLayout
          android:layout_width="match_parent"
          android:layout_height="44dp"
          android:layout_alignParentLeft="true"
          android:layout_alignParentTop="true"
          android:layout_marginTop="12dp"
          android:background="#FFFFFF"
          android:clipToOutline="true"
          android:elevation="5dp">

        <ImageView
            android:id="@+id/imageView13"
            android:layout_width="35dp"
            android:layout_height="32dp"
            app:srcCompat="@drawable/two_person" />

        <FrameLayout
            android:layout_width="285dp"
            android:layout_height="36dp"
            android:layout_marginLeft="8dp"
            android:layout_toRightOf="@id/imageView13">

          <TextView
              android:id="@+id/referrals"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:gravity="top"
              android:lineSpacingExtra="-3sp"
              android:text="@string/referrals"
              android:textAppearance="@style/referrals" />

          <TextView
              android:id="@+id/check_no_of"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_gravity="bottom"
              android:lineSpacingExtra="2sp"
              android:text="@string/check_no_of"
              android:textAppearance="@style/check_no_of"
              android:translationY="-0.97sp" />
        </FrameLayout>

        <ImageView
            android:id="@+id/imageView14"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="6dp"
            app:srcCompat="@drawable/frame_106" />
      </RelativeLayout>

      <RelativeLayout
          android:layout_width="match_parent"
          android:layout_height="44dp"
          android:layout_alignParentLeft="true"
          android:layout_alignParentTop="true"
          android:layout_marginTop="12dp"
          android:background="#FFFFFF"
          android:clipToOutline="true"
          android:elevation="5dp">

        <ImageView
            android:id="@+id/imageView15"
            android:layout_width="35dp"
            android:layout_height="32dp"
            app:srcCompat="@drawable/map" />

        <FrameLayout
            android:layout_width="285dp"
            android:layout_height="36dp"
            android:layout_marginLeft="8dp"
            android:layout_toRightOf="@id/imageView15">

          <TextView
              android:id="@+id/about_us"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:gravity="top"
              android:lineSpacingExtra="-3sp"
              android:text="@string/about_us"
              android:textAppearance="@style/about_us" />

          <TextView
              android:id="@+id/know_more_a"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_gravity="bottom"
              android:lineSpacingExtra="2sp"
              android:text="@string/know_more_a"
              android:textAppearance="@style/know_more_a"
              android:translationY="-0.97sp" />
        </FrameLayout>

        <ImageView
            android:id="@+id/imageView16"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="6dp"
            app:srcCompat="@drawable/frame_106" />
      </RelativeLayout>

      <RelativeLayout
          android:layout_width="match_parent"
          android:layout_height="44dp"
          android:layout_alignParentLeft="true"
          android:layout_alignParentTop="true"
          android:layout_marginTop="12dp"
          android:background="#FFFFFF"
          android:clipToOutline="true"
          android:elevation="5dp">

        <ImageView
            android:id="@+id/imageView17"
            android:layout_width="35dp"
            android:layout_height="32dp"
            android:paddingStart="4dp"
            android:paddingTop="4dp"
            app:srcCompat="@drawable/log_out" />

        <FrameLayout
            android:layout_width="285dp"
            android:layout_height="36dp"
            android:layout_marginLeft="8dp"
            android:layout_toRightOf="@id/imageView17">

          <TextView
              android:id="@+id/log_out"
              android:layout_width="wrap_content"
              android:layout_height="wrap_content"
              android:layout_gravity="center_vertical"
              android:lineSpacingExtra="-3sp"
              android:text="@string/log_out"
              android:textAppearance="@style/log_out" />
        </FrameLayout>

        <ImageView
            android:id="@+id/imageView18"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="6dp"
            app:srcCompat="@drawable/frame_106" />
      </RelativeLayout>
    </LinearLayout>


  </RelativeLayout>

  <LinearLayout
      android:id="@+id/nav_bar"
      android:layout_width="match_parent"
      android:layout_height="54dp"
      android:layout_alignParentBottom="true"
      android:background="#f1f1f1">


    <ImageButton
        android:id="@+id/button_home"
        android:layout_width="100dp"
        android:layout_height="match_parent"
        android:backgroundTint="@color/transparent_color"
        android:paddingStart="22dp"
        app:srcCompat="@drawable/home_unmain" />


    <ImageButton
        android:id="@+id/button_wallet"
        android:layout_width="100dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:backgroundTint="@color/transparent_color"
        android:paddingStart="22dp"
        app:srcCompat="@drawable/wallet" />

    <ImageButton
        android:id="@+id/button_track"
        android:layout_width="100dp"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:backgroundTint="@color/transparent_color"
        app:srcCompat="@drawable/track" />

    <RelativeLayout
        android:layout_width="100dp"
        android:layout_height="match_parent"
        android:paddingStart="12dp">

      <ImageView
          android:id="@+id/imageView4"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:paddingStart="8dp"
          app:srcCompat="@drawable/rectangle" />

      <ImageButton
          android:id="@+id/button_profile"
          android:layout_width="wrap_content"
          android:layout_height="match_parent"
          android:backgroundTint="@color/transparent_color"
          app:srcCompat="@drawable/profile_main" />

    </RelativeLayout>
  </LinearLayout>

</RelativeLayout>