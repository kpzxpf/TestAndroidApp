<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ChatRider">

    <RelativeLayout
        android:id="@+id/frame_56"
        android:layout_width="match_parent"
        android:layout_height="108dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:clipToOutline="true"
        android:background="#FFFFFF"
        android:elevation="5dp">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="30dp"
            android:layout_marginBottom="10dp">

            <ImageButton
                android:id="@+id/back"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom"
                android:backgroundTint="@color/transparent_color"
                android:paddingStart="18dp"
                android:layout_centerVertical="true"
                android:paddingBottom="8dp"
                app:srcCompat="@drawable/arrow_square_right" />

            <RelativeLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerInParent="true"
                android:layout_marginBottom="10dp">

                <ImageView
                    android:id="@+id/imageView51"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:srcCompat="@drawable/frame_83" />
                <TextView
                    android:id="@+id/chinoso_jam"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/chinoso_jam"
                    android:layout_toRightOf="@+id/imageView51"
                    android:textAppearance="@style/chinoso_jam2"
                    android:lineSpacingExtra="0sp"
                    android:gravity="top"
                    />
            </RelativeLayout>

            <ImageButton
                android:id="@+id/imageView52"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="16dp"
                android:backgroundTint="@color/transparent_color"
                app:srcCompat="@drawable/phone" />
        </RelativeLayout>
    </RelativeLayout>

    <RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/frame_136"
        android:layout_width="220dp"
        android:layout_height="52dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="146dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="138dp"
        android:background="@drawable/frame_136">
        <TextView
            android:id="@+id/hello_pleas"
            android:layout_width="match_parent"
            android:layout_height="32dp"
            android:text="@string/hello_pleas"
            android:textAppearance="@style/hello_pleas"
            android:lineSpacingExtra="2sp"
            android:layout_marginHorizontal="10dp"
            android:layout_centerInParent="true"
            android:translationY="-0.97sp"
            android:gravity="top"
            />
    </RelativeLayout>

    <RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/frame_137"
        android:layout_width="180dp"
        android:layout_height="36dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="26dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="202dp"
        android:background="@drawable/frame_137">
        <TextView
            android:id="@+id/thank_you_f"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="@string/thank_you_f"
            android:textAppearance="@style/thank_you_f"
            android:lineSpacingExtra="2sp"
            android:layout_marginHorizontal="10dp"
            android:layout_centerInParent="true"
            android:translationY="-0.97sp"
            android:gravity="top"
            />
    </RelativeLayout>

    <RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/frame_138"
        android:layout_width="220dp"
        android:layout_height="52dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="26dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="250dp"
        android:background="@drawable/frame_137">
        <TextView
            android:id="@+id/i_am_alread"
            android:layout_width="200dp"
            android:layout_height="32dp"
            android:text="@string/i_am_alread"
            android:textAppearance="@style/i_am_alread"
            android:lineSpacingExtra="2sp"
            android:layout_marginHorizontal="10dp"
            android:layout_centerInParent="true"
            android:translationY="-0.97sp"
            android:gravity="top"
            />

    </RelativeLayout>

    <RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/frame_139"
        android:layout_width="145dp"
        android:layout_height="36dp"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="220dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="314dp"
        android:background="@drawable/frame_136">
        <TextView
            android:id="@+id/alright_i_w"
            android:layout_width="136dp"
            android:layout_height="16dp"
            android:text="@string/alright_i_w"
            android:textAppearance="@style/alright_i_w"
            android:lineSpacingExtra="2sp"
            android:layout_marginHorizontal="10dp"
            android:layout_centerInParent="true"
            android:translationY="-0.97sp"
            android:gravity="top"
            />
    </RelativeLayout>





    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_alignParentBottom="true">


        <RelativeLayout
            android:id="@+id/frame_32"
            android:layout_width="267dp"
            android:layout_height="40dp"
            android:layout_centerVertical="true"
            android:layout_alignParentStart="true"
            android:layout_marginStart="50dp"
            android:background="@drawable/frame_24">
            <EditText
                android:layout_width="220dp"
                android:layout_height="wrap_content"
                android:hint="@string/enter_messa"
                android:paddingStart="12dp"
                android:textAppearance="@style/enter_messa"
                />
            <TextView
                android:id="@+id/enter_messa"
                android:layout_width="93dp"
                android:layout_height="16dp"
                android:layout_alignParentLeft="true"
                android:layout_marginLeft="32dp"
                android:layout_centerVertical="true"
                android:text="@string/enter_messa"
                android:textAppearance="@style/enter_messa"
                android:lineSpacingExtra="0sp"
                android:gravity="center_vertical"
                />

            <ImageButton
                android:id="@+id/imageButton7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_alignParentEnd="true"
                app:srcCompat="@drawable/dictation"
                android:backgroundTint="@color/transparent_color"/>
        </RelativeLayout>

        <ImageButton
            android:id="@+id/imageButton8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            app:srcCompat="@drawable/triangle"
            android:layout_alignParentEnd="true"
            android:backgroundTint="@color/transparent_color"/>

        <ImageButton
            android:id="@+id/imageButton9"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:backgroundTint="@color/transparent_color"
            app:srcCompat="@drawable/emoji" />

    </RelativeLayout>

</RelativeLayout>